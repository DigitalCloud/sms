{
  "name": "digitalcloud/sms",
  "description": "a sms notifier package for laravel that supports multiple providers",
  "keywords": [
    "laravel",
    "package",
    "sms",
    "unifonic",
    "twilio"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "twilio/sdk": "^5.20"
  },
  "autoload": {
    "psr-4": {
      "Digitalcloud\\SMS\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Digitalcloud\\SMS\\ServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
